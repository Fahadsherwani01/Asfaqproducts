     <script>



        $(".opener").on("click", function () {
            $("#dialog").dialog("open");
        });
        $('.delbtn').click(function () {

            var srcc = $(this).data("url");

            var name = $(this).data("name");
            document.getElementById("name").innerHTML = name;

            $("#imgg").attr("src", srcc);
            $("#imgg").attr("data-url2", srcc);

            var imgggg = document.getElementById("imgg");
           


        });

        $("#cart").click(function () {
            //alert('hhdh');

            $.ajax({

                url: "/project/addToCart",
                datatype: "text",
                data: { 'count': $('#quantity').val(), 'url': $('#imgg').data("url2") },
                type: "GET",
                beforeSend: function () {
                    $("#close").prop("disabled", true);
                    $("#cart").attr("disabled", true);
                },
                success: function (data) {
                    alert("Added To Cart");
                    document.getElementById("close").click();
                    $("#close").prop("disabled", false);
                    $("#cart").attr("disabled", false);
                },
                error: function () {
                    alert("ERROR, Try Again");
                },
                complete: function () {
                    $("#quantity").attr("value", null);

                }
            });
         });

    </script>

